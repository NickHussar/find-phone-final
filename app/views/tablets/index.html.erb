    <th>dimensions</th>
    <th>material</th>
    <th>protection</th>
  </tr>

  <% @tablets.each do |tablet| %>
    <tr>
      <td><%= tablet.developer %></td>
      <td><%= tablet.model %></td>
      <td><%= tablet.description %></td>
      <td><%= tablet.m_year %></td>
      <td><%= tablet.resolution_x %></td>
      <td><%= tablet.resolution_y %></td>
      <td><%= tablet.diagonal %></td>
      <td><%= tablet.screen_type %></td>
      <td><%= tablet.dpi %></td>
      <td><%= tablet.multitouch %></td>
      <td><%= tablet.cpu %></td>
      <td><%= tablet.cores %></td>
      <td><%= tablet.frequency %></td>
      <td><%= tablet.gpu %></td>
      <td><%= tablet.inner_mem %></td>
      <td><%= tablet.ram %></td>
      <td><%= tablet.cam %></td>
      <td><%= tablet.megapixels %></td>
      <td><%= tablet.photo_res_x %></td>
      <td><%= tablet.photo_res_y %></td>
      <td><%= tablet.hdr %></td>
      <td><%= tablet.front_megapixels %></td>
      <td><%= tablet.video_res_x %></td>
      <td><%= tablet.video_res_y %></td>
      <td><%= tablet.fps %></td>
      <td><%= tablet.phone %></td>
