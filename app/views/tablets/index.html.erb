    <th>description</th>
    <th>m_year</th>
    <th>resolution_x</th>
    <th>resolution_y</th>
    <th>diagonal</th>
    <th>screen_type</th>
    <th>dpi</th>
    <th>multitouch</th>
    <th>cpu</th>
    <th>cores</th>
    <th>frequency</th>
    <th>gpu</th>
    <th>inner_mem</th>
    <th>ram</th>
    <th>cam</th>
    <th>megapixels</th>
