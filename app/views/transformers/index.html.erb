      <td><%= transformer.barometer %></td>
      <td><%= transformer.wireless_charge %></td>
      <td><%= transformer.fast_charge %></td>
      <td><%= transformer.stylus %></td>
      <td><%= transformer.interfaces %></td>
      <td><%= transformer.cards %></td>
      <td><%= transformer.battery %></td>
      <td><%= transformer.standby_time %></td>
      <td><%= transformer.os %></td>
      <td><%= transformer.color %></td>
      <td><%= transformer.weight %></td>
      <td><%= transformer.dimensions %></td>
      <td><%= transformer.material %></td>
      <td><%= transformer.protection %></td>
      <td><%= link_to 'Show', transformer_path(transformer) %></td>
      <td><%= link_to 'Edit', edit_transformer_path(transformer) %></td>
      <td><%= link_to 'Destroy', transformer_path(transformer),
              method: :delete,
              data: { confirm: 'Вы уверены?' } %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'Findphone', controller: 'transformers' %>
