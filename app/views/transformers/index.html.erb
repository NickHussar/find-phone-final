    <th>dimensions</th>
    <th>material</th>
    <th>protection</th>
  </tr>

  <% @transformers.each do |transformer| %>
    <tr>
      <td><%= transformer.developer %></td>
      <td><%= transformer.model %></td>
      <td><%= transformer.description %></td>
      <td><%= transformer.m_year %></td>
      <td><%= transformer.resolution_x %></td>
      <td><%= transformer.resolution_y %></td>
      <td><%= transformer.diagonal %></td>
      <td><%= transformer.screen_type %></td>
      <td><%= transformer.dpi %></td>
      <td><%= transformer.multitouch %></td>
      <td><%= transformer.cpu %></td>
      <td><%= transformer.cores %></td>
      <td><%= transformer.frequency %></td>
      <td><%= transformer.gpu %></td>
      <td><%= transformer.inner_mem %></td>
      <td><%= transformer.ram %></td>
      <td><%= transformer.cam %></td>
      <td><%= transformer.megapixels %></td>
      <td><%= transformer.photo_res_x %></td>
      <td><%= transformer.photo_res_y %></td>
      <td><%= transformer.hdr %></td>
      <td><%= transformer.video_res_x %></td>
      <td><%= transformer.video_res_y %></td>
      <td><%= transformer.fps %></td>
      <td><%= transformer.three_g %></td>
      <td><%= transformer.four_g %></td>
