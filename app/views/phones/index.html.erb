<main class="main">
  <div class="container">
    <div class="sorting">

    </div>
    <h2>Телефоны</h2>
    <section class="line">
      <div>
        <% @phones.each do |phone| %>
          <%= link_to phone_path(phone) do %>
            <article>
              <%= image_tag 'phones/'+phone.developer+'_'+phone.model, alt: phone.developer+' '+phone.model %>
              <p><%= phone.developer %> <%= phone.model %></p>
            </article>
          <% end %>
        <% end %>
      </div>
    </section>
  </div>
  <%= render 'layouts/footer' %>
</main>
